<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
  <Appenders>
   <Console name="Console" target="SYSTEM_OUT">
   <PatternLayout>
   <pattern>%-5level-> %logger (%F:%L) - %msg%n%throwable</pattern>
   </PatternLayout>
    </Console>
  
  </Appenders>
  <Loggers>
    <Logger name="IPS" level="INFO"/>
    <Root level="ALL">
      <AppenderRef ref="Console"/>
    </Root>
  </Loggers>
</Configuration>